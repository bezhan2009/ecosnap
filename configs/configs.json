{
  "log_params": {
    "log_directory": "logs",
    "log_info": "info.log",
    "log_error": "error.log",
    "log_warn": "warn.log",
    "log_debug": "debug.log",
    "max_size_megabytes": 10,
    "max_backups": 4,
    "max_age": 30,
    "compress": true,
    "local_time": true
  },
  "app_params": {
    "gin_mode": "release",
    "port_run": "6565",
    "server_url": "localhost",
    "server_name": "ActiveDailyApplications_service"
  },
  "cors": {
    "allow_origins": ["*"],
    "allow_methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"],
    "allow_headers": ["Origin", "Content-Type", "Authorization"],
    "expose_headers": ["Content-Length"],
    "allow_credentials": true
  },
  "postgres_params": {
    "host": "localhost",
    "port": "5432",
    "user": "postgres",
    "database": "eco_db",
    "sslmode": "disable"
  },
  "auth_params": {
    "jwt_secret_key": "",
    "jwt_ttl_minutes": 360
  }
}
